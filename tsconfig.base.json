{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext",
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "module": "ESNext",
    "esModuleInterop": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "Node",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
  },
}
